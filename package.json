{
  "name": "atom-gist-dev",
  "main": "./lib/atom-gist-dev",
  "version": "0.2.0",
  "description": "A package to facilitate easy, all-in-one Gist development for non-developers",
  "keywords": [
    "git",
    "github",
    "gist"
  ],
  "activationCommands": {
    "atom-workspace": [
      "atom-gist-dev:clone",
      "atom-gist-dev:pull",
      "atom-gist-dev:save",
      "atom-gist-dev:save-share"
    ]
  },
  "repository": "https://github.com/CenterForAssessment/atom-gist-dev",
  "license": "MIT",
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "readmeFilename": "README.md",
  "dependencies": {
    "atom-space-pen-views": "^2.0.5",
    "expand-home-dir": "0.0.2",
    "git-auto": "^1.0.3",
    "gity": "^1.0.5",
    "space-pen": "^5.1.1"
  }
}
